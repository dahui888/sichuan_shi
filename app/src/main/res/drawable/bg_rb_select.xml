<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_selected="true">
        <shape>
            <stroke android:width="1dp" android:color="@color/color_00b464" />
            <corners android:radius="5dp" />
            <solid android:color="@color/color_00b464" />
        </shape>
    </item>
    <!-- 标签的正常背景 -->
    <item>
        <shape>
            <stroke android:width="1dp" android:color="@color/color_f0f0f0" />
            <corners android:radius="5dp" />
            <solid android:color="@color/trance" />
        </shape>
    </item>
</selector>